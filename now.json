{
  "version": 2,
  "name": "react-router",
  "type": "static",
  "public": true,
  "static": {
    "public": "build",
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": { "distDir": "demo/dist" }
    }
  ]
}
