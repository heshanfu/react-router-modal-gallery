{
  "name": "react-router-modal-gallery",
  "version": "1.1.0",
  "description": "A simple package to create modal gallery with react-router",
  "author": "Onur Onder",
  "homepage": "https://onderonur.github.io/react-router-modal-gallery",
  "license": "MIT",
  "repository": {
    "type": "git",
